<form {{action "save" on="submit"}}>
  <div>
    <label>
      Itemcode
      {{input type="text" value=model.itemcode}}
    </label>
  </div>
  <div>
    <label>
      Itemname
      {{input type="text" value=model.itemname}}
    </label>
  </div>
  <div>
    <label>
      Createdate
      {{input type="text" value=model.createdate}}
    </label>
  </div>
  <div>
    <input type="submit" value="Save">
  </div>
</form>
