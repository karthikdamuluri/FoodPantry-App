<form {{action "save" on="submit"}}>
  <div>
    <label>
      Itemcode
      {{input type="text" value=model.itemcode}}
    </label>
  </div>
  <div>
    <label>
      Itemname
      {{input type="text" value=model.itemname}}
    </label>
  </div>
  <div class="col-sm-2">
    <label>
      Createdate
      {{pikaday-input value=model.cratedate onSelection=(action (mut model.createdate)) readonly="readonly" format="MM/DD/YYYY"}}
    </label>
  </div>
  <div>
    <input type="submit" value="Save">
  </div>
</form>
