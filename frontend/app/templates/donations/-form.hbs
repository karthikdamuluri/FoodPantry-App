<form {{action "save" on="submit"}}>
  <div>
    <label>
      Item
      {{input type="text" value=model.item}}
    </label>
  </div>
  <div>
    <label>
      Donor
      {{input type="text" value=model.donor}}
    </label>
  </div>
  <div>
    <label>
      Quantity
      {{input type="text" value=model.quantity}}
    </label>
  </div>
  <div>
    <label>
      Receiveddate
      {{input type="text" value=model.receiveddate}}
    </label>
  </div>
  <div>
    <input type="submit" value="Save">
  </div>
</form>
